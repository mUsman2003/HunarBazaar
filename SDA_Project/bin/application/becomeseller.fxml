				<MenuItem mnemonicParsing="false" onAction="#menuclicked"
					text="Cook" />
				<MenuItem mnemonicParsing="false" onAction="#menuclicked"
					text="Technician" />
				<MenuItem mnemonicParsing="false" onAction="#menuclicked"
					text="Mason" />
				<MenuItem mnemonicParsing="false" text="Carpenter" />
				<MenuItem mnemonicParsing="false" onAction="#menuclicked"
					text="Welder" />
				<MenuItem mnemonicParsing="false" onAction="#menuclicked"
					text="Tailor" />
				<MenuItem mnemonicParsing="false" text="Sewage Cleaner" />
				<MenuItem mnemonicParsing="false" onAction="#menuclicked"
					text="BlackSmith" />
				<MenuItem mnemonicParsing="false" text="Plasterer" />
				<MenuItem mnemonicParsing="false" text="Tiler" />
				<MenuItem mnemonicParsing="false" onAction="#menuclicked"
					text="Locksmith" />
				<MenuItem mnemonicParsing="false" onAction="#menuclicked"
					text="Painter" />
				<MenuItem mnemonicParsing="false" onAction="#menuclicked"
					text="Pest Control" />
