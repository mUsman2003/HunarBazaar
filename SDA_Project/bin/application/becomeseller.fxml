			prefHeight="33.0" prefWidth="272.0" />
		<TextField fx:id="cnic" layoutX="778.0" layoutY="395.0"
			prefHeight="33.0" prefWidth="272.0" />
		<Button fx:id="signup" layoutX="846.0" layoutY="478.0"
			mnemonicParsing="false" onAction="#gotosellerdash" opacity="0.0"
			prefHeight="45.0" prefWidth="124.0" text="Button" />
		<Button fx:id="goback" layoutX="23.0" layoutY="24.0"
			mnemonicParsing="false" onAction="#gotoback" opacity="0.0"
			prefHeight="51.0" prefWidth="67.0" text="Button">
			<cursor>
				<Cursor fx:constant="HAND" />
			</cursor>
		</Button>
	</children>
</Pane>
